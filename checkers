      <script type = "text/javascript" language="javascript">
	  
	  
	 $(document).ready(function() {
            $('#play').click(function() {
			
				$("#play").hide("slow");
				
				$("#menu").hide("slow");
				
				$("#insertName").show("slow"); /*,function(){alert("Prepare Yourself");
				});*/
				
				//$("#quit").show("slow");
				
			});
			
			$('#submit').click(function() {
			
				var player1 = document.getElementById("p1").value;
				var player2 = document.getElementById("p2").value;
			
				$("#insertName").hide("slow");
				
				//$("#menu").hide("slow");
				
				$("#displayName1").text(player1+" you are Red");
				$("#displayName2").text(player2+" you are Blue");

				

				
				$("#board").show("slow"); /*,function(){alert("Prepare Yourself");
				});*/
				
				$("#quit").show("slow");
				
			});
				
			
			
			$("#quit").click(function(){
				$("#board").hide("slow");/*, function(){alert("Quitter!");});*/
				$("#quit").hide("slow");
				$("#sure").show("slow");
				$("#displayName").hide("slow");
				//$("#play").show("slow");
				
				//$("#menu").show("slow");
				
				//quit();
				//location.reload();
			});
			
			$("#yes").click(function(){
				$("#box").hide("slow", function(){alert("Quitter!");
				});
				$("#quit").hide("slow");
				
				$("#play").show("slow");
				
				$("#menu").show("slow");
				
				location.reload();
			});
			
			
			$("#no").click(function(){
			
				$("#sure").hide("slow");
				$("#displayName").hide("slow");
				$("#board").show("slow");
				$("#quit").show("slow");
				
			});
			
			
			
		/*	$("#number1").one('click', function() {
				Record();
			});*/


			
			
			
		});

var drag_rIndex = " ";
var drag_cIndex = " ";
var drop_rIndex = " ";
var drop_cIndex = " ";
//var ev = "1";
var redCnt = 0;
var blueCnt = 0;

	  function drag(ev)
{
	//console.log("1" + drag_Indexes.drag_rIndex + " | " + drag_Indexes.drag_cIndex + " | " + drop_Indexes.drop_rIndex + " | " + drop_Indexes.drop_cIndex);
	//console.log("1 " + drag_rIndex + " | " + drag_cIndex + " | " + drop_rIndex + " | " + drop_cIndex);

		ev.dataTransfer.setData("Text",ev.target.id);
		//ev.originalEvent.dataTransfer("Text",ev.target.id);
		//ev.dataTransfer.effectAllowed("Text",ev.target.id);

		//Row Index
		drag_rIndex = ev.target.parentNode.parentNode.rowIndex+1;
	    console.log("Row index is: " + drag_rIndex);
		
		//Column Index
		drag_cIndex = ev.target.parentNode.cellIndex+1;
		console.log("Cell index is: " + drag_cIndex);
		
/*Only drop to certain spots*/

		//legal moves from pos1
		if(drag_rIndex == 1 && drag_cIndex == 1){
			console.log("pos 1");
			
			var t = document.getElementById("board");
			var trs = t.getElementsByTagName("tr");
			var tds = null;

			for (var i=0; i<trs.length; i++)
			{
				tds = trs[i].getElementsByTagName("td");
				
				for (var n=0; n<trs.length;n++)
				{
					tds[n].setAttribute("ondragover", " ");
					tds[n].setAttribute("ondrop", " ");
				}
			}
			
			//var test = document.getElementById("pos10").hasChildNodes();
			//console.log(test);
			var pos1 = document.getElementById("pos1");
			var pos10 = document.getElementById("pos10");

			var child1 = pos1.firstElementChild;
			var child10 = pos10.firstElementChild;
			
			var king = child1.className;
			
			var test1 = pos1.contains(child1);
			var test10 = pos10.contains(child10);
			//console.log(test1);
			//console.log(test10);
			
			if(test1 == true && test10 == true){
			
				var color1 = pos1.firstElementChild.alt;
				var color10 = pos10.firstElementChild.alt;
			
				//console.log(color1);
				//console.log(color10);
			
				if(color1 == "Red" && color10 == "Blue" || king == "blueKing" && color10 == "Red"){
					//console.log(color1);
					//console.log(color10);
					//console.log(king+"1");
					document.getElementById("pos19").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos19").setAttribute("ondrop", "drop(event)");
				}else if(color1 == "Red" || king == "blueKing"){
					document.getElementById("pos10").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos10").setAttribute("ondrop", "drop(event)");
				}
			
			}else if(test1 == true && test10 == false){
				
				var color1 = pos1.firstElementChild.alt;
				//console.log("test");
				if(color1 == "Red" || king == "blueKing"){
					//console.log(king+"2");
					document.getElementById("pos10").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos10").setAttribute("ondrop", "drop(event)");
				}
			}
			
			
		
		}
		
		
		
		//legal moves from pos3
		if(drag_rIndex == 1 && drag_cIndex == 3){
			console.log("pos 3");
			
			var t = document.getElementById("board");
			var trs = t.getElementsByTagName("tr");
			var tds = null;

			for (var i=0; i<trs.length; i++)
			{
				tds = trs[i].getElementsByTagName("td");
				
				for (var n=0; n<trs.length;n++)
				{
					tds[n].setAttribute("ondragover", " ");
					tds[n].setAttribute("ondrop", " ");
				}
			}
			
			//var test = document.getElementById("pos10").hasChildNodes();
			//console.log(test);
			
			var pos3 = document.getElementById("pos3");
			var pos10 = document.getElementById("pos10");
			var pos17 = document.getElementById("pos17");
			var pos12 = document.getElementById("pos12");
			var pos21 = document.getElementById("pos21");
			
			var child3 = pos3.firstElementChild;
			var child10 = pos10.firstElementChild;
			var child17 = pos17.firstElementChild;
			var child12 = pos12.firstElementChild;
			var child21 = pos21.firstElementChild;
			var king = child3.className;
			
			var test3 = pos3.contains(child3);
			var test10 = pos10.contains(child10);
			var test17 = pos17.contains(child17);
			var test12 = pos12.contains(child12);
			var test21 = pos21.contains(child21);
			//console.log(test1);
			//console.log(test10);
			
			if(test3 == true && test10 == true){
			
				var color3 = pos3.firstElementChild.alt;
				var color10 = pos10.firstElementChild.alt;
			
				//console.log(color3);
				//console.log(color10);
			
				if(color3 == "Red" && color10 == "Blue"  || king == "blueKing" && color10 == "Red"){
					//console.log(color3);
					//console.log(color10);
					document.getElementById("pos17").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos17").setAttribute("ondrop", "drop(event)");
				}else if(color3 == "Red" || king == "blueKing"){
					document.getElementById("pos10").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos10").setAttribute("ondrop", "drop(event)");
				}
			
			}else if(test3 == true && test10 == false){
				var color3 = pos3.firstElementChild.alt;

				if(color3 == "Red"  || king == "blueKing"){
					document.getElementById("pos10").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos10").setAttribute("ondrop", "drop(event)");
				}
			}
			
			if(test3 == true && test12 == true){
			
				var color3 = pos3.firstElementChild.alt;
				var color12 = pos12.firstElementChild.alt;
			
				//console.log(color3);
				//console.log(color12);
			
				if(color3 == "Red" && color12 == "Blue" || king == "blueKing" && color12 == "Red"){
					//console.log(color3);
					//console.log(color12);
					document.getElementById("pos21").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos21").setAttribute("ondrop", "drop(event)");
				}else if(color3 == "Red" || king == "blueKing"){
					document.getElementById("pos12").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos12").setAttribute("ondrop", "drop(event)");
				}
			
			}else if(test3 == true && test12 == false){
				var color3 = pos3.firstElementChild.alt;

				if(color3 == "Red" || king == "blueKing"){
					document.getElementById("pos12").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos12").setAttribute("ondrop", "drop(event)");
				}
			}
			
		
		}
		
		
		
		
		//legal moves from pos5
		if(drag_rIndex == 1 && drag_cIndex == 5){
			console.log("pos 5");
			
			var t = document.getElementById("board");
			var trs = t.getElementsByTagName("tr");
			var tds = null;

			for (var i=0; i<trs.length; i++)
			{
				tds = trs[i].getElementsByTagName("td");
				
				for (var n=0; n<trs.length;n++)
				{
					tds[n].setAttribute("ondragover", " ");
					tds[n].setAttribute("ondrop", " ");
				}
			}
			
			//var test = document.getElementById("pos10").hasChildNodes();
			//console.log(test);
			
			var pos5 = document.getElementById("pos5");
			var pos12 = document.getElementById("pos12");
			var pos19 = document.getElementById("pos19");
			var pos14 = document.getElementById("pos14");
			var pos23 = document.getElementById("pos23");
			
