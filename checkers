					document.getElementById("pos55").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos55").setAttribute("ondrop", "drop(event)");
				}
			}
			
		
		}

//legal moves from pos64
		if(drag_rIndex == 8 && drag_cIndex == 8){
			console.log("pos 64");
			
			var t = document.getElementById("board");
			var trs = t.getElementsByTagName("tr");
			var tds = null;

			for (var i=0; i<trs.length; i++)
			{
				tds = trs[i].getElementsByTagName("td");
				
				for (var n=0; n<trs.length;n++)
				{
					tds[n].setAttribute("ondragover", " ");
					tds[n].setAttribute("ondrop", " ");
				}
			}
			
			//var test = document.getElementById("pos55").hasChildNodes();
			//console.log(test);
			var pos64 = document.getElementById("pos64");
			var pos55 = document.getElementById("pos55");
			
