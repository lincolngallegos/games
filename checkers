				}*/
			
			}
			
			if(test62 == true && test53 == false){
				var color62 = pos62.firstElementChild.alt;
				
				if(color62 == "Blue" || king == "redKing"){
					document.getElementById("pos53").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos53").setAttribute("ondrop", "drop(event)");
				}
			}
			
			if(test62 == true && test55 == false){
				var color62 = pos62.firstElementChild.alt;
				
