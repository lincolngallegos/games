			var child55 = pos55.firstElementChild;
			
			var king = child64.className;
			
			var test64 = pos64.contains(child64);
			var test55 = pos55.contains(child55);
			//console.log(test64);
			//console.log(test55);
			
			if(test55 == true && test64 == true){
			
				var color64 = pos64.firstElementChild.alt;
				var color55 = pos55.firstElementChild.alt;
			
				//console.log(color64);
				//console.log(color55);
			
				if(color64=="Blue" && color55=="Red" || king == "redKing" && color55 == "Blue"){
					//console.log(color64);
					//console.log(color55);
					document.getElementById("pos46").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos46").setAttribute("ondrop", "drop(event)");
				}else if(color64=="Blue" || king == "redKing"){
					document.getElementById("pos55").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos55").setAttribute("ondrop", "drop(event)");
				}
			
			}else if(test55 == false && test64 == true){
				var color64 = pos64.firstElementChild.alt;
				
				if(color64=="Blue" || king == "redKing"){
					document.getElementById("pos55").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos55").setAttribute("ondrop", "drop(event)");
					/*document.getElementById("pos46").setAttribute("ondragover", " ");
					document.getElementById("pos46").setAttribute("ondrop", " ");*/
				}
			}
			
		
		}


		
		
}
	  


function drop(ev)
{
    var id = ev.dataTransfer.getData("Text");
    if (!$(ev.currentTarget).find("img")[0]){
		ev.target.appendChild(document.getElementById(id));
			
			console.log("dropped");

			//Row Index
		drop_rIndex = ev.target.parentNode.rowIndex+1;
	    console.log("Row index is: " + drop_rIndex);
		
		//Column Index
		drop_cIndex = ev.target.cellIndex+1;
		console.log("Cell index is: " + drop_cIndex);
			console.log("indexes: " + drag_rIndex + " | " + drag_cIndex + " | " + drop_rIndex + " | " + drop_cIndex);
			
			
			
		/*KING*/
		//blueKing
