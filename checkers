			var test62 = pos62.contains(child62);
			var test55 = pos55.contains(child55);
			var test48 = pos48.contains(child48);
			var test53 = pos53.contains(child53);	
			var test44 = pos44.contains(child44);

			//console.log(test62);
			//console.log(test10);
			
			if(test62 == true && test55 == true){
			
				var color62 = pos62.firstElementChild.alt;
				var color55 = pos55.firstElementChild.alt;
			
				//console.log(color62);
				//console.log(color55);
			
				if(color62 == "Blue" && color55 == "Red" || king == "redKing" && color55 == "Blue"){
					//console.log(color62);
					//console.log(color55);
					document.getElementById("pos48").setAttribute("ondragover", "allowDrop(event)");
					document.getElementById("pos48").setAttribute("ondrop", "drop(event)");
				}/*else if(color62 == "Blue"){
					document.getElementById("pos55").setAttribute("ondragover", "allowDrop(event)");
